{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Sql": "#{SqlConnectionString}#"
  },
  "Spotify": {
    "ClientId": "#{ClientId}#",
    "ClientSecret": "#{ClientSecret}#",
    "CallbackUri": "#{CallbackUri}#",
    "ServerUri": "#{ServerUri}#",
    "DisableMetadataFetching": false
  },
  "Import": {
    "MaxConsecutiveFailures": 3
  }
}
